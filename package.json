{
  "name": "xms-ts",
  "version": "1.0.6",
  "description": "TypeScript implementation of the XMS (exchange metadata standard) with strict parser, fallback parser, and type-safe XMSDoc wrapper.",
  "license": "MIT",
  "author": "Ivan Oliver Fabra <ivan@iofabra.cc>",
  "homepage": "https://github.com/ivanoliverfabra/xms-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanoliverfabra/xms-ts.git"
  },
  "bugs": {
    "url": "https://github.com/ivanoliverfabra/xms-ts/issues"
  },
  "keywords": [
    "xms",
    "metadata",
    "parser",
    "typescript",
    "serializer",
    "fallback",
    "protocol"
  ],
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.build.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "postbuild:cjs": "echo {\"type\":\"commonjs\"} > dist/cjs/package.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run postbuild:cjs",
    "test": "bun test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}